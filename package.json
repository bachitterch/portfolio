{
  "name": "bachitter.dev",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:server": "wrangler dev ./src/server/index.ts",
    "dev:client": "vite --host",
    "dev": "run-p dev:server dev:client",
    "build": "tsc && vite build",
    "preview": "vite preview && pnpm build",
    "start": "run-p preview dev:server",
    "lint": "eslint . --cache --cache-strategy content --ignore-path .gitignore",
    "lint:fix": "eslint . --fix --ignore-path .gitignore",
    "format": "prettier --write '**/*.{ts,tsx,cjs,mjs,json,md,mdx}'",
    "fix": "pnpm lint:fix && pnpm format"
  },
  "dependencies": {
    "@apollo/client": "^3.7.3",
    "@tanstack/react-query": "^4.20.4",
    "@tanstack/react-router": "0.0.1-beta.38",
    "@trpc/client": "^10.6.0",
    "@trpc/react-query": "^10.6.0",
    "@trpc/server": "^10.6.0",
    "dayjs": "^1.11.7",
    "graphql": "^16.6.0",
    "hono": "^2.6.2",
    "miniflare": "^2.11.0",
    "motion": "^10.15.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20221111.1",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "@vitejs/plugin-react": "^3.0.0",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.20",
    "prettier": "^2.8.1",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.4",
    "vite": "^4.0.3",
    "wrangler": "^2.6.2",
    "zod": "^3.20.2"
  }
}
